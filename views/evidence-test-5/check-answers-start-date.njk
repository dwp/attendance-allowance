{% from "./macros/check-answers-start-date-section.njk" import checkAnswersStartDateSection %}
{% from "govuk/components/table/macro.njk" import govukTable %}

{% extends "v5-layouts/prototypeLayout.njk" %}


{% block form %}
  {% if data['supporting-documents-options'] == 'upload-now' %}
    <a href="supporting-documents-added" class="govuk-back-link">Back</a>
  {% elif data['supporting-documents-options'] == 'upload-later' %}
    <a href="supporting-documents-email-address" class="govuk-back-link">Back</a>
  {% elif data['supporting-documents-options'] == 'post' %}
    <a href="supporting-documents-post" class="govuk-back-link">Back</a>
  {% else %}
    <a href="evidence" class="govuk-back-link">Back</a>
  {% endif %}


    <h1 class="govuk-heading-l">
        Check your answers
    </h1>
        
      {{ checkAnswersStartDateSection(data) }}

{% endblock %}

{% block formButton %}
  <a href="claim-submission" class="govuk-button">
    Accept and continue
  </a>
{% endblock %}

{% block postForm %}
    {% include './macros/assisted-digital-details.njk' %}
{% endblock %}