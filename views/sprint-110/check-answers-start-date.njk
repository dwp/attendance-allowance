{% from "./macros/check-answers-start-date-section.njk" import checkAnswersStartDateSection %}
{% from "govuk/components/table/macro.njk" import govukTable %}

{% extends "v5.9.0-layouts/prototypeLayout.njk" %}


{% block form %}
  {% if data['aids-adaptations-question'] == "yes" %}
    <a href="aids-adaptations-added" class="govuk-back-link">Back</a>
  {% else %}
    <a href="aids-adaptations-reason" class="govuk-back-link">Back</a>
  {% endif %}
    
  <h1 class="govuk-heading-l">
      Check your answers
  </h1>
      
  {{ checkAnswersStartDateSection(data) }}

{% endblock %}

{% block formButton %}
  <a href="telephone-number" class="govuk-button">
    Accept and continue
  </a>
{% endblock %}

{% block postForm %}
    {% include './macros/assisted-digital-details.njk' %}
{% endblock %}